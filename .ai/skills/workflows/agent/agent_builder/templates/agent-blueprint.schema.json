{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.local/schemas/agent-blueprint.schema.json",
  "title": "Agent Blueprint",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "kind",
    "version",
    "meta",
    "agent",
    "scope",
    "integration",
    "interfaces",
    "schemas",
    "model",
    "configuration",
    "acceptance",
    "deliverables"
  ],
  "properties": {
    "kind": {
      "const": "agent_blueprint"
    },
    "version": {
      "type": "integer",
      "minimum": 1
    },
    "meta": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "generated_at"
      ],
      "properties": {
        "generated_at": {
          "type": "string",
          "format": "date-time"
        },
        "generated_by": {
          "type": "string"
        },
        "source": {
          "type": "string"
        },
        "notes": {
          "type": "string"
        }
      }
    },
    "agent": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "id",
        "name",
        "summary",
        "owners"
      ],
      "properties": {
        "id": {
          "type": "string",
          "minLength": 1
        },
        "name": {
          "type": "string",
          "minLength": 1
        },
        "summary": {
          "type": "string",
          "minLength": 1
        },
        "owners": {
          "type": "array",
          "minItems": 1,
          "items": {
            "$ref": "#/$defs/Owner"
          }
        }
      }
    },
    "scope": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "in_scope",
        "out_of_scope",
        "definition_of_done"
      ],
      "properties": {
        "in_scope": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string",
            "minLength": 1
          }
        },
        "out_of_scope": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string",
            "minLength": 1
          }
        },
        "definition_of_done": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "integration": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "primary",
        "attach",
        "trigger",
        "target",
        "upstream_contract_ref",
        "downstream_contract_ref",
        "failure_contract",
        "rollback_or_disable"
      ],
      "properties": {
        "primary": {
          "type": "string",
          "enum": [
            "api"
          ]
        },
        "attach": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "worker",
              "sdk",
              "cron",
              "pipeline"
            ]
          },
          "uniqueItems": true
        },
        "trigger": {
          "$ref": "#/$defs/Trigger"
        },
        "target": {
          "$ref": "#/$defs/Target"
        },
        "upstream_contract_ref": {
          "$ref": "#/$defs/SchemaRef"
        },
        "downstream_contract_ref": {
          "$ref": "#/$defs/SchemaRef"
        },
        "failure_contract": {
          "$ref": "#/$defs/FailureContract"
        },
        "rollback_or_disable": {
          "$ref": "#/$defs/Rollback"
        }
      }
    },
    "interfaces": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "#/$defs/Interface"
      }
    },
    "schemas": {
      "type": "object",
      "minProperties": 1,
      "required": [
        "RunRequest",
        "RunResponse",
        "AgentError"
      ],
      "properties": {
        "RunRequest": {
          "$ref": "#/$defs/JsonSchemaLike"
        },
        "RunResponse": {
          "$ref": "#/$defs/JsonSchemaLike"
        },
        "AgentError": {
          "$ref": "#/$defs/JsonSchemaLike"
        }
      },
      "additionalProperties": {
        "$ref": "#/$defs/JsonSchemaLike"
      }
    },
    "model": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "primary"
      ],
      "properties": {
        "primary": {
          "$ref": "#/$defs/ModelConfig"
        },
        "fallback": {
          "anyOf": [
            {
              "$ref": "#/$defs/ModelConfig"
            },
            {
              "type": "null"
            }
          ]
        },
        "profiles": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/$defs/ModelConfig"
          }
        }
      }
    },
    "configuration": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "env_vars"
      ],
      "properties": {
        "env_vars": {
          "type": "array",
          "minItems": 1,
          "items": {
            "$ref": "#/$defs/EnvVar"
          }
        },
        "config_files": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/ConfigFile"
          }
        }
      }
    },
    "acceptance": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "scenarios"
      ],
      "properties": {
        "scenarios": {
          "type": "array",
          "minItems": 3,
          "items": {
            "$ref": "#/$defs/AcceptanceScenario"
          }
        }
      }
    },
    "deliverables": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "agent_module_path",
        "docs_path",
        "registry_path",
        "core_adapter_separation"
      ],
      "properties": {
        "agent_module_path": {
          "type": "string",
          "minLength": 1
        },
        "docs_path": {
          "type": "string",
          "minLength": 1
        },
        "registry_path": {
          "type": "string",
          "minLength": 1
        },
        "core_adapter_separation": {
          "const": "required"
        },
        "non_goals": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "api": {
      "$ref": "#/$defs/ApiConfig"
    },
    "worker": {
      "$ref": "#/$defs/WorkerConfig"
    },
    "sdk": {
      "$ref": "#/$defs/SdkConfig"
    },
    "cron": {
      "$ref": "#/$defs/CronConfig"
    },
    "pipeline": {
      "$ref": "#/$defs/PipelineConfig"
    },
    "tools": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "tools": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Tool"
          }
        }
      }
    },
    "data_flow": {
      "$ref": "#/$defs/DataFlow"
    },
    "observability": {
      "$ref": "#/$defs/Observability"
    },
    "operations": {
      "$ref": "#/$defs/Operations"
    },
    "prompting": {
      "$ref": "#/$defs/Prompting"
    },
    "security": {
      "$ref": "#/$defs/Security"
    },
    "lifecycle": {
      "$ref": "#/$defs/Lifecycle"
    }
  },
  "allOf": [
    {
      "if": {
        "properties": {
          "integration": {
            "properties": {
              "primary": {
                "const": "api"
              }
            },
            "required": [
              "primary"
            ]
          }
        }
      },
      "then": {
        "required": [
          "api"
        ],
        "properties": {
          "interfaces": {
            "type": "array",
            "allOf": [
              {
                "contains": {
                  "properties": {
                    "type": {
                      "const": "http"
                    }
                  },
                  "required": [
                    "type"
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "if": {
        "properties": {
          "integration": {
            "properties": {
              "attach": {
                "contains": {
                  "const": "worker"
                }
              }
            }
          }
        }
      },
      "then": {
        "required": [
          "worker"
        ],
        "properties": {
          "interfaces": {
            "type": "array",
            "allOf": [
              {
                "contains": {
                  "properties": {
                    "type": {
                      "const": "worker"
                    }
                  },
                  "required": [
                    "type"
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "if": {
        "properties": {
          "integration": {
            "properties": {
              "attach": {
                "contains": {
                  "const": "sdk"
                }
              }
            }
          }
        }
      },
      "then": {
        "required": [
          "sdk"
        ],
        "properties": {
          "interfaces": {
            "type": "array",
            "allOf": [
              {
                "contains": {
                  "properties": {
                    "type": {
                      "const": "sdk"
                    }
                  },
                  "required": [
                    "type"
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "if": {
        "properties": {
          "integration": {
            "properties": {
              "attach": {
                "contains": {
                  "const": "cron"
                }
              }
            }
          }
        }
      },
      "then": {
        "required": [
          "cron"
        ],
        "properties": {
          "interfaces": {
            "type": "array",
            "allOf": [
              {
                "contains": {
                  "properties": {
                    "type": {
                      "const": "cron"
                    }
                  },
                  "required": [
                    "type"
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "if": {
        "properties": {
          "integration": {
            "properties": {
              "attach": {
                "contains": {
                  "const": "pipeline"
                }
              }
            }
          }
        }
      },
      "then": {
        "required": [
          "pipeline"
        ],
        "properties": {
          "interfaces": {
            "type": "array",
            "allOf": [
              {
                "contains": {
                  "properties": {
                    "type": {
                      "const": "pipeline"
                    }
                  },
                  "required": [
                    "type"
                  ]
                }
              }
            ]
          }
        }
      }
    }
  ],
  "$defs": {
    "Owner": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "type",
        "id"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "person",
            "team",
            "service"
          ]
        },
        "id": {
          "type": "string",
          "minLength": 1
        },
        "contact": {
          "type": "string"
        }
      }
    },
    "Trigger": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "kind"
      ],
      "properties": {
        "kind": {
          "type": "string",
          "enum": [
            "sync_request",
            "async_event",
            "scheduled",
            "manual",
            "batch"
          ]
        },
        "notes": {
          "type": "string"
        }
      }
    },
    "Target": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "kind",
        "name"
      ],
      "properties": {
        "kind": {
          "type": "string",
          "enum": [
            "service",
            "repo_module",
            "pipeline_step",
            "queue",
            "topic",
            "job",
            "function",
            "other"
          ]
        },
        "name": {
          "type": "string",
          "minLength": 1
        },
        "path": {
          "type": "string"
        },
        "owner_hint": {
          "type": "string"
        }
      }
    },
    "SchemaRef": {
      "type": "string",
      "pattern": "^#/schemas/[A-Za-z0-9_]+$"
    },
    "FailureContract": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "mode"
      ],
      "properties": {
        "mode": {
          "type": "string",
          "enum": [
            "propagate_error",
            "return_fallback",
            "enqueue_retry"
          ]
        },
        "notes": {
          "type": "string"
        }
      }
    },
    "Rollback": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "method"
      ],
      "properties": {
        "method": {
          "type": "string",
          "enum": [
            "feature_flag",
            "config_toggle",
            "route_switch",
            "deployment_rollback"
          ]
        },
        "key": {
          "type": "string"
        },
        "notes": {
          "type": "string"
        }
      }
    },
    "Interface": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "type",
        "entrypoint",
        "input_schema_ref",
        "output_schema_ref",
        "examples_min"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "http",
            "worker",
            "sdk",
            "cron",
            "pipeline",
            "cli"
          ]
        },
        "entrypoint": {
          "type": "string",
          "minLength": 1
        },
        "input_schema_ref": {
          "$ref": "#/$defs/SchemaRef"
        },
        "output_schema_ref": {
          "$ref": "#/$defs/SchemaRef"
        },
        "error_schema_ref": {
          "$ref": "#/$defs/SchemaRef"
        },
        "examples_min": {
          "type": "integer",
          "minimum": 1
        },
        "notes": {
          "type": "string"
        }
      }
    },
    "ModelConfig": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "model",
        "reasoning_profile"
      ],
      "properties": {
        "provider": {
          "type": "object",
          "additionalProperties": true,
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "openai",
                "openai_compatible",
                "azure_openai",
                "internal_gateway",
                "local"
              ]
            },
            "base_url_env_var": {
              "type": "string"
            },
            "api_key_env_var": {
              "type": "string"
            },
            "notes": {
              "type": "string"
            }
          }
        },
        "model": {
          "type": "string",
          "minLength": 1
        },
        "reasoning_profile": {
          "type": "string",
          "minLength": 1
        },
        "temperature": {
          "type": "number"
        },
        "max_output_tokens": {
          "type": "integer",
          "minimum": 1
        },
        "timeout_ms": {
          "type": "integer",
          "minimum": 1
        }
      }
    },
    "EnvVar": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "name",
        "description",
        "required",
        "sensitivity",
        "example_placeholder"
      ],
      "properties": {
        "name": {
          "type": "string",
          "pattern": "^[A-Z][A-Z0-9_]*$"
        },
        "description": {
          "type": "string",
          "minLength": 1
        },
        "required": {
          "type": "boolean"
        },
        "sensitivity": {
          "type": "string",
          "enum": [
            "public",
            "internal",
            "secret"
          ]
        },
        "example_placeholder": {
          "type": "string"
        }
      }
    },
    "ConfigFile": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "path",
        "purpose"
      ],
      "properties": {
        "path": {
          "type": "string",
          "minLength": 1
        },
        "purpose": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "AcceptanceScenario": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "title",
        "given",
        "when",
        "then",
        "expected_output_checks",
        "priority"
      ],
      "properties": {
        "title": {
          "type": "string",
          "minLength": 1
        },
        "given": {
          "type": "string",
          "minLength": 1
        },
        "when": {
          "type": "string",
          "minLength": 1
        },
        "then": {
          "type": "string",
          "minLength": 1
        },
        "expected_output_checks": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string",
            "minLength": 1
          }
        },
        "priority": {
          "type": "string",
          "enum": [
            "P0",
            "P1",
            "P2"
          ]
        }
      }
    },
    "ApiConfig": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "protocol",
        "base_path",
        "timeout_budget_ms",
        "routes",
        "auth",
        "degradation"
      ],
      "properties": {
        "protocol": {
          "type": "string",
          "enum": [
            "http",
            "grpc"
          ]
        },
        "base_path": {
          "type": "string",
          "minLength": 1
        },
        "timeout_budget_ms": {
          "type": "integer",
          "minimum": 1
        },
        "routes": {
          "type": "array",
          "minItems": 2,
          "items": {
            "$ref": "#/$defs/ApiRoute"
          },
          "allOf": [
            {
              "contains": {
                "properties": {
                  "name": {
                    "const": "run"
                  }
                },
                "required": [
                  "name"
                ]
              }
            },
            {
              "contains": {
                "properties": {
                  "name": {
                    "const": "health"
                  }
                },
                "required": [
                  "name"
                ]
              }
            }
          ]
        },
        "auth": {
          "type": "object",
          "additionalProperties": true,
          "required": [
            "kind"
          ],
          "properties": {
            "kind": {
              "type": "string",
              "enum": [
                "none",
                "api_key",
                "bearer_token",
                "oauth2",
                "mtls",
                "internal_gateway"
              ]
            },
            "env_var": {
              "type": "string"
            },
            "notes": {
              "type": "string"
            }
          }
        },
        "degradation": {
          "type": "object",
          "additionalProperties": true,
          "required": [
            "mode"
          ],
          "properties": {
            "mode": {
              "type": "string",
              "enum": [
                "none",
                "return_fallback",
                "return_unavailable",
                "route_to_worker"
              ]
            },
            "notes": {
              "type": "string"
            }
          }
        }
      }
    },
    "ApiRoute": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "name",
        "method",
        "path",
        "request_schema_ref",
        "response_schema_ref"
      ],
      "properties": {
        "name": {
          "type": "string",
          "enum": [
            "run",
            "health"
          ]
        },
        "method": {
          "type": "string",
          "enum": [
            "get",
            "post",
            "put",
            "patch",
            "delete"
          ]
        },
        "path": {
          "type": "string",
          "minLength": 1
        },
        "request_schema_ref": {
          "$ref": "#/$defs/SchemaRef"
        },
        "response_schema_ref": {
          "$ref": "#/$defs/SchemaRef"
        },
        "error_schema_ref": {
          "$ref": "#/$defs/SchemaRef"
        },
        "notes": {
          "type": "string"
        }
      }
    },
    "WorkerConfig": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "source",
        "execution",
        "retry",
        "idempotency",
        "failure"
      ],
      "properties": {
        "source": {
          "type": "object",
          "additionalProperties": true,
          "required": [
            "kind",
            "name"
          ],
          "properties": {
            "kind": {
              "type": "string",
              "enum": [
                "queue",
                "topic",
                "task_table",
                "cron",
                "webhook"
              ]
            },
            "name": {
              "type": "string",
              "minLength": 1
            },
            "notes": {
              "type": "string"
            }
          }
        },
        "execution": {
          "type": "object",
          "additionalProperties": true,
          "required": [
            "max_concurrency",
            "timeout_ms"
          ],
          "properties": {
            "max_concurrency": {
              "type": "integer",
              "minimum": 1
            },
            "timeout_ms": {
              "type": "integer",
              "minimum": 1
            }
          }
        },
        "retry": {
          "type": "object",
          "additionalProperties": true,
          "required": [
            "max_attempts",
            "backoff"
          ],
          "properties": {
            "max_attempts": {
              "type": "integer",
              "minimum": 1
            },
            "backoff": {
              "type": "object",
              "additionalProperties": true,
              "required": [
                "strategy"
              ],
              "properties": {
                "strategy": {
                  "type": "string",
                  "enum": [
                    "fixed",
                    "exponential",
                    "exponential_jitter"
                  ]
                },
                "base_delay_ms": {
                  "type": "integer",
                  "minimum": 0
                }
              }
            }
          }
        },
        "idempotency": {
          "type": "object",
          "additionalProperties": true,
          "required": [
            "strategy"
          ],
          "properties": {
            "strategy": {
              "type": "string",
              "enum": [
                "none",
                "header",
                "payload_field",
                "hash_payload",
                "external_key"
              ]
            },
            "key_ref": {
              "type": "string"
            },
            "notes": {
              "type": "string"
            }
          }
        },
        "failure": {
          "type": "object",
          "additionalProperties": true,
          "required": [
            "dead_letter",
            "alert_on"
          ],
          "properties": {
            "dead_letter": {
              "type": "object",
              "additionalProperties": true,
              "required": [
                "kind"
              ],
              "properties": {
                "kind": {
                  "type": "string",
                  "enum": [
                    "none",
                    "queue",
                    "topic",
                    "table"
                  ]
                },
                "name": {
                  "type": "string"
                }
              }
            },
            "alert_on": {
              "type": "string",
              "enum": [
                "always",
                "after_retries",
                "never"
              ]
            }
          }
        }
      }
    },
    "SdkConfig": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "language",
        "package",
        "exports",
        "compatibility"
      ],
